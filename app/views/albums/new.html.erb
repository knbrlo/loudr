<h1>Create New Album</h1>

<%= form_for(@album) do |f| %>
    <%= render partial: 'layouts/errors', locals: {object: @album} %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :category %>
    <%= f.text_field :category %>
    <%= f.label :release_date %>
    <%= f.text_field :release_date %>
    <%= f.label :relased %>
    <%= f.check_box :released %>
    <%= f.submit "Create Album" %>
<% end %>