<div id="header-container">
    <div id="header-container-logo">
        
    </div>
    <div id="header-container-buttons">
        <% if logged_in_creator? || logged_in_user? %>
            <% if logged_in_creator? %>
                <%= link_to 'Home', home_creator_path %>
                <%= link_to "New Album", new_album_path %>       
                <%= link_to 'Log Out', logout_creator_path, method: :delete, class: 'nav-item'%>
            <% elsif logged_in_user? %>
                <%= link_to 'Home', home_path %>
                <%= link_to 'Log Out', logout_path, method: :delete, class: 'nav-item'%>
            <% end%>
        <% else %>
            <% if for_creator_url? %>
                <%= link_to "Get Access", signup_creator_path %>
                <%= link_to "Log In", login_creator_path %>
            <% else %>
                <%= link_to "Sign Up", signup_path %>
                <%= link_to "Log In", login_path %>
            <% end %>
        <% end %>
    </div>
</div>