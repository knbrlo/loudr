<h1>User Login Page</h1>
<%= flash[:error] %>
<%= link_to "Continue with Google",  "/auth/google_oauth2", method: "post" %>
<%= form_for @user, url: '/login' do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Login" %>
<% end %>